                        <ul>
                            @if (User.Identity.IsAuthenticated)
                            {
                                <li><a class="nav-link scrollto" asp-action="AddEditAccident" asp-route-crashid="0">Add New Crash</a></li>
                            }
                            <li><a class="nav-link scrollto" asp-controller="Home" asp-action="Score">Score a Crash</a></li>
                            <li><a class="nav-link scrollto" asp-controller="Home" asp-action="Privacy">Privacy Policy</a></li>
